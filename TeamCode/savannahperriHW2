     // enumerations (enum) represent a group of constants
     enum Color {
        BLUE,
        RED,
        GREEN
      }

      public class Main {
        public static void main(String[] args) {
          Color myVar = Color.GREEN;
          System.out.println(myVar);
        }
      }

     // int (short for integer) represents whole numbers like 20 or 50 and has no decimals or fractions
    public class Main {
      public static void main(String[] args) {
        int firstNumber = 151;
            int secondNumber = 120;

            System.out.println(firstNumber);
            System.out.println(secondNumber);

            int sum = firstNumber + secondNumber;

            System.out.println(sum);
      }
    }

    // double represents fraction numbers or numbers with decimals
    public class Main {
      public static void main(String[] args) {
        double myNum = 1.2345d;
        System.out.println(myNum);
      }
    }

     // string is a sequence of characters and are in double quotes
     public class Main {
       public static void main(String[] args) {
         string greeting = "Hello World";
         System.out.println(greeting);
       }
     }
     // constants have fixed values that cannot be changed
     package main
     import ("fmt")

     const A int = 2

     func main() {
       fmt.Println(A)
     }

     // Direct current (dc) motor converts current electrical energy into mechanical energy

     // Servo motors control motions



